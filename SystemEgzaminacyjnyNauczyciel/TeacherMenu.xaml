<Window x:Class="SystemEgzaminacyjny.TeacherMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SystemEgzaminacyjny"
        mc:Ignorable="d"
        Title="Menu nauczyciela" Height="190" Width="200" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="NewButton" Content="Nowy egzamin" HorizontalAlignment="Center" Margin="0,29,0,0" VerticalAlignment="Top" FontSize="16" Height="30" Width="120" Click="NewButton_Click"/>
        <Button x:Name="ViewButton" Content="Zobacz wyniki" HorizontalAlignment="Center" Margin="0,69,0,0" VerticalAlignment="Top" FontSize="16" Height="30" Width="120" Click="ViewButton_Click"/>
        <Button x:Name="EndButton" Content="Zakończ" HorizontalAlignment="Center" Margin="0,109,0,0" VerticalAlignment="Top" FontSize="16" Height="30" Width="120" Click="EndButton_Click"/>
    </Grid>
</Window>
